{
  "expo": {
    "name": "systematic",
    "slug": "systematic",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.xjaku.systematic"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.xjaku.systematic"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/auth",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "images/dummy-black-image.png",
          "resizeMode": "contain"
        }
      ],
      "expo-font",
      "expo-system-ui",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b7eece7f-fb4f-4c02-b79e-86410c012af6"
      }
    },
    "owner": "xjaku",
    "updates": {
      "url": "https://u.expo.dev/b7eece7f-fb4f-4c02-b79e-86410c012af6"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
